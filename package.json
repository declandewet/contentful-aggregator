{
  "name": "contentful-aggregator",
  "version": "0.0.0-semantic-release",
  "description": "functional contentful configuration for static site generators",
  "main": "lib",
  "scripts": {
    "test": "standard && nyc --reporter=lcov babel-tape-runner test/**/*.js | tap-spec",
    "coverage": "nyc report",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src -d lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Declan de Wet <declandewet@me.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "blue-tape": "^0.1.11",
    "cz-conventional-changelog": "^1.1.5",
    "nyc": "^5.5.0",
    "semantic-release": "^4.3.5",
    "standard": "^5.4.1",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "common-tags": "0.0.1",
    "contentful": "^2.1.2",
    "ramda": "^0.19.1",
    "ramda-fantasy": "^0.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/declandewet/contentful-aggregator.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
